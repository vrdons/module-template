{
	"name": "@vrdons/module-template",
	"version": "0.0.4",
	"description": "Module template",
	"homepage": "https://github.com/vrdons/module-template#readme",
	"bugs": {
		"url": "https://github.com/vrdons/module-template/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vrdons/module-template.git"
	},
	"license": "MIT",
	"author": "vrdons",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"prepare": "node ./scripts/husky/installHusky.mjs",
		"lint": "biome check",
		"lint:fix": "biome check --write --unsafe",
		"build": "tsup",
		"changelog": "node ./scripts/actions/generateChangelog.mjs",
		"release:git": "node ./scripts/actions/githubRelease.mjs",
		"release:npm": "node ./scripts/actions/npmRelease.mjs"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/lint": "^19.8.1",
		"@commitlint/load": "^19.8.1",
		"@octokit/rest": "^22.0.0",
		"@swc/core": "^1.13.3",
		"@types/node": "^24.1.0",
		"@typescript-eslint/parser": "^8.38.0",
		"husky": "^9.1.7",
		"libnpmpack": "^9.0.6",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"tslib": "^2.8.1"
	}
}
